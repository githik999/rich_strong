{
    "manifest_version": 2,
    "name": "rich strong",
    "description": "show website's ip and country flag.show latency.proxy control.etc",
    "version": "1.0",

    "icons": {"64": "image/logo.png"},

    "background": {
        "scripts": [
            "background/china_route.js",
            "background/ip_manager.js",
            "background/domain_manager.js",
            "background/main.js"
        ]
    },

    "page_action": {
        "default_icon": "image/logo.png"
    },

    "permissions": ["proxy", "dns", "<all_urls>"]
}